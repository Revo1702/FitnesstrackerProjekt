        <TextBlock HorizontalAlignment="Left" Margin="183,155,0,0" TextWrapping="Wrap" Text="Größe" VerticalAlignment="Top" Height="23" Width="90"/>
        <TextBlock HorizontalAlignment="Left" Margin="183,185,0,0" TextWrapping="Wrap" Text="Gewicht" VerticalAlignment="Top" Height="23" Width="90"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="325,65,0,0" TextWrapping="Wrap" Text="name" VerticalAlignment="Top" Width="120" RenderTransformOrigin="0.19,0.426"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="325,95,0,0" TextWrapping="Wrap" Text="nachname" VerticalAlignment="Top" Width="120"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="325,125,0,0" TextWrapping="Wrap" Text="alter" VerticalAlignment="Top" Width="120"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="325,155,0,0" TextWrapping="Wrap" Text="größe" VerticalAlignment="Top" Width="120"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="325,185,0,0" TextWrapping="Wrap" Text="gewicht" VerticalAlignment="Top" Width="120"/>

    </Grid>
